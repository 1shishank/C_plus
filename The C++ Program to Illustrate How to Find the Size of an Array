#include<iostream>
using namespace std;
int main(){
    int table [10]=
    {
        1,2,3,4,5,6,7,8,9,9
    };
    for(int i =0 ; i<10;i++){
        cout<< table[i]<<endl;
    }
    cout <<sizeof(table)<<endl;
    cout <<sizeof(table[0])<<endl;
     cout <<"size of array "<< sizeof(table) / sizeof(table[0])<<endl;
    
}
